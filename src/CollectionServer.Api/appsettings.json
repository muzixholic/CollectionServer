{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=collectionserver;Username=postgres;Password=postgres;Maximum Pool Size=100;Connection Idle Lifetime=300;Connection Pruning Interval=10"
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowSeconds": 60,
    "QueueLimit": 10
  },
  "ExternalApis": {
    "GoogleBooks": {
      "ApiKey": "",
      "BaseUrl": "https://www.googleapis.com/books/v1",
      "Priority": 1,
      "TimeoutSeconds": 10
    },
    "KakaoBook": {
      "ApiKey": "",
      "BaseUrl": "https://dapi.kakao.com",
      "Priority": 2,
      "TimeoutSeconds": 10
    },
    "AladinApi": {
      "ApiKey": "",
      "BaseUrl": "http://www.aladin.co.kr/ttb/api",
      "Priority": 1,
      "TimeoutSeconds": 10
    },
    "TMDb": {
      "ApiKey": "",
      "BaseUrl": "https://api.themoviedb.org/3",
      "Priority": 1,
      "TimeoutSeconds": 10
    },
    "OMDb": {
      "ApiKey": "",
      "BaseUrl": "http://www.omdbapi.com",
      "Priority": 2,
      "TimeoutSeconds": 10
    },
    "MusicBrainz": {
      "BaseUrl": "https://musicbrainz.org/ws/2",
      "UserAgent": "CollectionServer/1.0",
      "Priority": 1,
      "TimeoutSeconds": 10
    },
    "Discogs": {
      "ApiKey": "",
      "ApiSecret": "",
      "BaseUrl": "https://api.discogs.com",
      "Priority": 2,
      "TimeoutSeconds": 10
    }
  },
  "Monitoring": {
    "OtlpExporter": {
      "Endpoint": ""
    }
  }
}
